class FireSensor is subclass of Sensor

--
-- Operations definition section
--

instance variables

operations

public FireSensor: Utils`Type * int * AlarmControl ==> FireSensor
	FireSensor (t, val, a) == (
		type := t;
		value := val;
		alarm := a;
		return self
		);

public setValue: nat ==> ()
	setValue(v) ==
		value := v;

end FireSensor